<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
    <p></p>
</div>
<div id="body">
    @foreach (News news in Model)
    {
        <div class="jumbotron">
            <h5><a href="@news.Link" target="_blank">@news.Title</a></h5>
            <h6>@news.Description</h6>
        </div>
        if (news.ImageUrl != null)
        {
            <img src="@news.ImageUrl">
        }
    }
</div>